<style>
  {% capture content %}
  {% comment %}
  @font-face {
    font-family: 'Font Name';
      src: url("{{ 'file-name.woff' | asset_url }}") format('woff'),
           url("{{ 'file-name.woff2' | asset_url }}") format('woff2');
    font-weight: 400;
    font-style: normal;
    font-display: swap;
  }
  {% endcomment %}
  {% endcapture %}

  {{ content | strip_newlines | strip }}
</style>
